{
  "name": "collector",
  "version": "0.1.0",
  "private": false,
  "description": "Manage your euro collection",
  "author": "Flibustier",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint src/extractor/*.mjs",
    "format": "prettier src/**/* --write",
    "test": "jest",
    "extract": "node --experimental-modules src/extractor/index.mjs",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'"
  },
  "dependencies": {
    "buefy": "^0.8.9",
    "c8r": "^1.0.2",
    "core-js": "^3.4.4",
    "vue": "^2.6.11",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@fortawesome/fontawesome-pro": "^5.12.0",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.2",
    "@vue/cli-service": "^4.1.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-vue": "^5.0.0",
    "flag-icon-css": "^3.4.5",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "progress": "^2.0.3",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-store": "^0.0.7",
    "vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
    "vue-template-compiler": "^2.6.11",
    "yargs": "^15.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "vue/no-unused-components": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "MIT"
}